if (COPO.utility.urlParam("redirect")) {
  window.location.replace(COPO.utility.urlParam("redirect"));
}
$('#approved').html("<%= j (render partial: 'approved', locals: {approved: @approved, devices: @devices}) %>")
<% if @pending.present? %>
  $('#pending').html("<%= j (render partial: 'pending', locals: {pending: @pending}) %>")
<% else %>
  $('#pending').remove();
<% end %>
$('.modal-trigger').leanModal();
COPO.permissions.check_disabled();
COPO.permissions.switch_change();
